@model CodeFirstVenueBooking.Models.CustomerEventDetails


<h2> Edit </h2>

<form asp-action="Edit" asp-controller="CustomerEventDetails" method="post">
    <div class="form-group">
        <label asp-for="EventName" class="form-control col-sn-2">Enter Name</label>
        <input asp-for="EventName" class="form-control col-sn-10" readonly="true" />
    </div>
    <div class="form-group">
        <label asp-for="People" class="form-control col-sn-2">Enter For How Many People</label>
        <input asp-for="People" class="form-control col-sn-10" />
    </div>
    <div class="form-group">
        <label asp-for="EventDate" class="form-control col-sn-2">Event Date</label>
        <input asp-for="EventDate" class="form-control col-sn-10" />
    </div>
    <div class="form-group">
        <label asp-for="Details" class="form-control col-sn-2">Enter Details</label>
        <input asp-for="Details" class="form-control col-sn-10" />
    </div>
    <div class="form-group">
        <label asp-for="UserName" class="form-control col-sn-2">Enter UserName</label>
        <input asp-for="UserName" class="form-control col-sn-10" />
    </div>

    <div>
        <button type="submit" class=" btb btn-success">Update</button>
    </div>

</form>